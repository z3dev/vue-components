{
  "name": "vue-components",
  "version": "0.0.0",
  "description": "VUE components for JSCAD",
  "repository": "https://github.com/jscad/OpenJSCAD.org/",
  "main": "src/index.js",
  "scripts": {
    "build": "browserify src/viewerComponent.js -p tinyify -o lib/viewerComponent.js --standalone viewerComponent",
    "coverage": "nyc --all --reporter=html --reporter=text npm test",
    "test": "ava --verbose --timeout 2m 'tests/**/*.test.js'"
  },
  "keywords": [
    "jscad",
    "vue"
  ],
  "license": "MIT",
  "dependencies": {
    "@jscad/regl-renderer": "alpha",
    "@jscad/modeling": "alpha"
  },
  "devDependencies": {
    "ava": "3.10.0",
    "nyc": "15.1.0",
    "browserify": "16.5.1",
    "tinyify": "3.0.0"
  }
}
